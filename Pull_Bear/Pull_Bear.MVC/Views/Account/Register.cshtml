@model RegisterVM

<main>

    <section class="whereareyou">
        <div class="where">.</div>
    </section>

    <section class="breadcrumbregsiter">
        <div class="container">
            <div class="row keeper">
                <div class="col-lg-6">
                    <a asp-controller="Home" asp-action="Index">MAIN PAGE</a>&nbsp;/ REGISTER
                </div>
            </div>
        </div>
    </section>

    <section class="registersection">
        <div class="container">
            <div class="row all flex-wrap">
                <p class="firstp">REGISTER</p>
                <div asp-validation-summary="ModelOnly" class="text-danger text col-lg-7 col-12 text-center"></div>
                <form method="post" action="" class="col-lg-6 col-12 ">
                    <label asp-for="Email" for="email"><span>*</span> EMAIL</label>
                    <input asp-for="Email" id="email" class="col-12 col-lg-12" required type="email">
                    <span asp-validation-for="Name" class="text-danger col-lg-12 col-12"></span>

                    <div class="passworddiv col-12 col-lg-12">
                        <label asp-for="Password" for="password"><span>*</span> PASSWORD</label>
                        <input asp-for="Password" id="password" class="col-12 col-lg-12" required type="password">
                        <span asp-validation-for="Password" class="text-danger col-lg-12 col-12"></span>

                        <span class="col-lg-12 col-12 infopass">
                            8 characters, 1 lowercase, 1 uppercase, 1 number
