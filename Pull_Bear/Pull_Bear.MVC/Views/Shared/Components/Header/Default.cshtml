                                MAN
                            </a>
                        </li>
                    </ul>
                    <ul class="headermenu col-6">
                        <li class="col-6 home">
                            <a asp-controller="Shop" asp-action="Index">
                                SHOP
                            </a>
                        </li>
                        <li class="col-3 home">
                            <a asp-controller="Home" asp-action="Index" class="homehref">
                                <ion-icon name="home-outline" class="sidebar_btn"></ion-icon>
                            </a>
                        </li>
                        <li class="col-3 close">
                            <ion-icon name="close-outline" class="sidebar_btn close_btn"></ion-icon>
                        </li>
                    </ul>
                </div>

                <div class="tab_container">
                    <ul id="tab1" class="tab_content">
                        @foreach (CategoryListVM category in Model.Categories.Where(x => x.GenderId == 1))
                        {
                            <li class="sidebar_menu_li">
                                <div class="imagekeeper col-2">
                                    <img src="~/assets/images/categories/@category.Image" class="img-fluid" alt="">
                                </div>
                                <a data-rel="woman" asp-controller="Shop" asp-action="Index" asp-route-parentcategoryid="@category.Id" asp-route-genderid="1" class="col-9">@category.Name.ToUpperInvariant()</a>
                            </li>
                        }
                    </ul>
                    <ul id="tab2" class="tab_content">
                        @foreach (CategoryListVM category in Model.Categories.Where(x => x.GenderId == 2))
                        {
                            <li class="sidebar_menu_li">
                                <div class="imagekeeper col-2">
                                    <img src="~/assets/images/categories/@category.Image" class="img-fluid" alt="">
                                </div>
                                <a data-rel="man" asp-controller="Shop" asp-action="Index" asp-route-parentcategoryid="@category.Id" asp-route-genderid="2" class="col-9">@category.Name.ToUpperInvariant()</a>
                            </li>
                        }
                    </ul>
