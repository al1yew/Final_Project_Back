                    <ul class="tab-menu col-6">
                        <li data-rel="tab1" class="active-tab woman col-6 bcgrgrey">
                            <a class="sidebar_ahref" asp-controller="Shop" asp-action="Index">
                                WOMAN
                            </a>
                        </li>
                        <li data-rel="tab2" class="col-6 man bcgrgrey">
